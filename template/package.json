    "@vue/test-utils": "^1.0.0-beta.31",
    "@wemake-services/eslint-config-javascript": "^0.4.2",
    "@wemake-services/eslint-config-jest": "^0.2.2",
    "@wemake-services/eslint-config-jsdoc": "^0.4.2",
    "@wemake-services/eslint-config-typescript": "^0.4.1",
    "@wemake-services/eslint-config-vue": "^0.2.2",
    "@wemake-services/stylelint-config-scss": "^0.2.2",
    "axios-mock-adapter": "^1.17.0",
    "babel-core": "^6.26.3",
    "check-engine": "^1.8.1",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-compat": "^3.5.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-import-helpers": "^1.0.2",
    "eslint-plugin-jest": "^23.8.0",
    "eslint-plugin-jsdoc": "^21.0.0",
    "eslint-plugin-json": "^2.1.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-nuxt": "^0.5.1",
