{
  "name": "{{ name }}",
  "version": "0.1.0",
  "description": "{{ description }}",
  "author": "{{ author }}",
  "license": "MIT",
  "main": "client/index.js",
  "private": true,
  "engines": {
    "node": "^8.8.0",
    "npm": "^5.4.2"
  },
  "scripts": {
    "postinstall": "flow-typed install",
    "test:security": "nsp check",
    "test:e2e": "cross-env NODE_ENV=test node tests/e2e/runner.js",
    "test:unit": "cross-env NODE_ENV=test jest --no-cache tests/unit",
    "test": "npm run test:unit && npm run test:security && npm run lint",
    "clean": "rimraf dist",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.prod.js",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.js",
    "build": "npm run clean && npm run build:client && npm run build:server",
    "ci": "npm run build && npm run test",
    "dev": "cross-env NODE_ENV=development node build/dev-server.js",
    "start": "cross-env NODE_ENV=production node server",
    "lint:js": "eslint --ext .js,.vue,.json client server tests build",
    "lint:css": "stylelint '**/*.{vue,scss}' --syntax scss",
    "lint": "npm run lint:js && npm run lint:css"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "cssnano": {},
      "css-mqpacker": {}
    }
  },
  "browserslist": [
    "last 2 versions",
    "ie > 8"
  ],
  "stylelint": {
    "plugins": [
      "stylelint-scss"
    ],
    "processors": [
      "stylelint-processor-html"
    ],
    "extends": "stylelint-config-standard",
    "rules": {
      "no-empty-source": null
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "collectCoverage": true,
    "verbose": true,
    "globals": {
      "NODE_ENV": "test"
    },
    "moduleDirectories": [
      "node_modules",
      "client"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.vue$": "vue-jest"
    }
  },
  "dependencies": {
    "await-to-js": "^2.0.1",
    "axios": "^0.17.1",
    "babel-runtime": "^6.18.0",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "lru-cache": "^4.1.1",
    "promise-polyfill": "^6.0.2",
    "vue": "^2.5.3",
    "vue-meta": "^1.2.0",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.4",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "ajv": "^5.3.0",
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.16.0",
    "babel-eslint": "^8.0.2",
    "babel-helper-vue-jsx-merge-props": "^2.0.2",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-vue-app": "^1.3.2",
    "babel-register": "^6.26.0",
    "chalk": "^2.3.0",
    "chromedriver": "^2.33.2",
    "compression-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.1.1",
    "cross-spawn": "^5.1.0",
    "css-loader": "^0.28.7",
    "css-mqpacker": "^6.0.1",
    "cssnano": "^3.10.0",
    "eslint": "^4.11.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-flowtype-errors": "^3.3.6",
    "eslint-plugin-html": "^4.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-unicorn": "^3.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^0.9.0",
    "flow-bin": "^0.59.0",
    "flow-typed": "^2.2.1",
    "friendly-errors-webpack-plugin": "^1.1.2",
    "html-webpack-plugin": "^2.22.0",
    "jest": "^21.2.1",
    "nightwatch": "^0.9.16",
    "node-sass": "^4.7.1",
    "nsp": "^3.1.0",
    "postcss-loader": "^2.0.8",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "selenium-server": "^3.7.1",
    "style-loader": "^0.19.0",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-processor-html": "^1.0.0",
    "stylelint-scss": "^2.1.0",
    "stylelint-webpack-plugin": "^0.9.0",
    "vue-jest": "^1.0.3",
    "vue-loader": "^13.5.0",
    "vue-ssr-webpack-plugin": "^3.0.0",
    "vue-template-compiler": "^2.5.6",
    "vue-test-utils": "^1.0.0-beta.3",
    "webpack": "^3.8.1",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-dev-server": "^2.9.4",
    "webpack-hot-middleware": "^2.20.0",
    "webpack-merge": "^4.1.1"
  }
}
