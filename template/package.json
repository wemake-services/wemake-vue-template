{
  "name": "{{ name }}",
  "version": "0.0.0",
  "description": "{{ description }}",
  "author": "{{ author }}",
  "private": true,
  "engines": {
    "node": "10.15",
    "npm": ">=6"
  },
  "scripts": {
    "clean": "rm -rf .nuxt dist",
    "dev": "cross-env NODE_ENV=development nuxt",
    "build": "npm run clean && cross-env NODE_ENV=production nuxt build",
    "start": "cross-env NODE_ENV=production nuxt start",
    "generate": "cross-env NODE_ENV=production nuxt generate",
    "test:engine": "check-engine",
    "test:sec": "npm audit || true",
    "test:unit": "cross-env NODE_ENV=test jest",
    "test": "npm run test:engine && npm run test:unit && npm run lint",
    "lint:js": "eslint --ext .js,.ts,.vue,.json --ignore-path .gitignore .",
    "lint:css": "stylelint 'client/**/*.{vue,scss,css}'",
    "lint:md": "remark -q -f -r docs/.remarkrc.js .",
    "lint": "npm run lint:js && npm run lint:css && npm run lint:md",
    "docs:api": "documentation build client/** -f md -o docs/project/api.md",
    "docs:serve": "npm run docs:build && gitbook serve docs",
    "docs:build": "npm run docs:api && gitbook build docs",
    "ci": "npm run test && npm run build && npm run test:sec"
  },
  "browserslist": "> 5%, last 2 versions, not ie < 11, not ie_mob < 11, not dead",
  "dependencies": {
    "@nuxtjs/axios": "5.4.1",
    "cross-env": "^5.2.0",
    "dotenv": "^8.0.0",
    "node-sass": "^4.12.0",
    "normalize.css": "^8.0.1",
    "nuxt": "^2.8.0",
    "nuxt-class-component": "^1.3.0",
    "nuxt-imagemin": "^1.0.0",
    "nuxt-property-decorator": "2.1.3",
    "postcss": "^7.0.16",
    "sass-loader": "^7.1.0",
    "ts-node": "^8.1.0",
    "vuex-class": "0.3.2"
  },
  "devDependencies": {
    "@nuxt/typescript": "^2.8.0",
    "@nuxt/vue-app": "^2.8.0",
    "@types/faker": "^4.1.5",
    "@types/jest": "^24.0.12",
    "@types/rosie": "0.0.36",
    "@types/webpack-env": "1.13.9",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "@wemake-services/eslint-config-javascript": "^0.1.5",
    "@wemake-services/eslint-config-jest": "^0.1.5",
    "@wemake-services/eslint-config-jsdoc": "^0.1.5",
    "@wemake-services/eslint-config-vue": "^0.1.4",
    "@wemake-services/stylelint-config-scss": "^0.1.4",
    "axios-mock-adapter": "^1.16.0",
    "babel-core": "^6.26.3",
    "check-engine": "^1.8.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-compat": "^3.1.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-jsdoc": "^7.2.3",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-nuxt": "^0.4.3",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-sonarjs": "^0.3.0",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-testcafe": "^0.2.1",
    "eslint-plugin-unicorn": "^9.0.0",
    "eslint-plugin-vue": "^5.2.2",
