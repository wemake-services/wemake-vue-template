{
  "name": "{{ name }}",
  "version": "0.0.0",
  "description": "{{ description }}",
  "author": "{{ author }}",
  "private": true,
  "engines": {
    "node": "12.14",
    "npm": ">=6"
  },
  "scripts": {
    "clean": "rm -rf .nuxt dist",
    "dev": "cross-env NODE_ENV=development nuxt-ts",
    "build": "npm run clean && cross-env NODE_ENV=production nuxt-ts build",
    "start": "cross-env NODE_ENV=production nuxt-ts start",
    "generate": "cross-env NODE_ENV=production nuxt-ts generate",
    "test:engine": "check-engine",
    "test:sec": "npm run test:sec:audit && npm run test:sec:lockfile",
    "test:sec:lockfile": "lockfile-lint --type npm --path package-lock.json --validate-https --allowed-hosts npm",
    "test:sec:audit": "npm audit || true",
    "test:unit": "cross-env NODE_ENV=test jest",
    "test": "npm run test:engine && npm run test:unit && npm run lint",
    "lint:js": "eslint --cache --ext .js,.ts,.vue,.json --ignore-path .gitignore .",
    "lint:css": "stylelint 'client/**/*.{vue,scss,css}' && stylelint --rd 'client/**/*.{vue,scss,css}'",
    "lint:md": "remark -q -f -r docs/.remarkrc.js .",
    "lint:dup": "jscpd -g --threshold=1 client tests docker",
    "lint": "npm run lint:js && npm run lint:dup && npm run lint:css && npm run lint:md",
    "docs:api": "documentation build client/** -f md -o docs/project/api.md",
    "docs:serve": "npm run docs:build && gitbook serve docs",
    "docs:build": "npm run docs:api && gitbook build docs",
    "ci": "npm run test && npm run build && npm run test:sec"
  },
  "browserslist": [
    "> 5%",
    "last 2 versions",
    "not ie < 11",
    "not ie_mob < 11",
    "not dead"
  ],
  "dependencies": {
    "@nuxt/typescript-runtime": "^0.3.9",
    "@nuxtjs/axios": "5.9.5",
    "cross-env": "^7.0.0",
    "dotenv": "^8.2.0",
    "io-ts": "^1.10.4",
    "io-ts-promise": "^1.1.1",
    "io-ts-types": "^0.4.8",
    "monocle-ts": "^1.7.2",
    "node-sass": "^4.13.1",
    "normalize.css": "^8.0.1",
    "nuxt": "^2.11.0",
    "nuxt-class-component": "^1.3.0",
    "nuxt-imagemin": "^1.0.0",
    "nuxt-property-decorator": "2.5.1",
    "nuxt-vuex-router-sync": "0.0.3",
    "postcss": "^7.0.27",
    "sass-loader": "^8.0.2",
    "vue-typedi": "^1.1.1",
    "vuex-class": "0.3.2",
    "vuex-simple": "^2.2.0"
  },
